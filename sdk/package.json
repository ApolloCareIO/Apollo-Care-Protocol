{
  "name": "@apollo-care/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Apollo Care Protocol on Solana",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "solana",
    "blockchain",
    "healthcare",
    "insurance",
    "defi",
    "dao",
    "apollo-care"
  ],
  "author": "Apollo Care Protocol",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ApolloCareIO/apollo-care-protocol.git",
    "directory": "sdk"
  },
  "bugs": {
    "url": "https://github.com/ApolloCareIO/apollo-care-protocol/issues"
  },
  "homepage": "https://apollocare.io",
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.91.0",
    "bn.js": "^5.2.1"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.91.0"
  },
  "engines": {
    "node": ">=18"
  }
}
